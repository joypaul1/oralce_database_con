
CREATE SEQUENCE DEVELOPERS.HR_SLIDER_IMAGE_SEC
START WITH 1
INCREMENT BY 1
NOCACHE;

CREATE OR REPLACE TRIGGER DEVELOPERS.HR_SLIDER_IMAGE_TRIGG 
BEFORE INSERT ON DEVELOPERS.HR_SLIDER_IMAGE
FOR EACH ROW
BEGIN
  SELECT HR_SLIDER_IMAGE_SEC.NEXTVAL
  INTO   :new.id
  FROM   dual;
END;
/


--DROP SEQUENCE HR_SLIDER_IMAGE_SEC;
--DROP TRIGGER DEVELOPERS.HR_SLIDER_IMAGE_TRIGG;